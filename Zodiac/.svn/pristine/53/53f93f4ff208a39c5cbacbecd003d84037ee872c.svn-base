                        }
                        if (fitem.SO_release_date != null)
                        {
                            item.SO_Release_Date = fitem.SO_release_date;
                        }
                        if (fitem.ETD != null)
                        {
                            item.ETD = fitem.ETD;
                        }
                        if (fitem.Shipment_Status != null || fitem.Shipment_Status != "")
                        {
                            item.Shipment_Status = fitem.Shipment_Status;
                        }
                        if (fitem.NLT != null)
                        {
