                        if (trkingitem.CBM != null)
                        {
                            item.CBM = trkingitem.CBM;
                        }
                        if (trkingitem.Booking_Number != null || trkingitem.Booking_Number != "")
                        {

                            item.TruckingNo = trkingitem.Booking_Number;
                        }
                    }
                }


            }
        }

