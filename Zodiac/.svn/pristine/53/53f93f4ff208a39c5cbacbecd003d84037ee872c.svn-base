            int total = 0;
            var roles = GeAllMIGOReportForIQueryable(Customer_name, DateType, operDateStart, operDateEnd, out total, page, rows,
               Purch_Doc, Vendor_Name, Item, sort, order);

            return new DataGrid
            {
