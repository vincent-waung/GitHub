        public static int getHoliday(int day,List<FinalLoadPlanHoliday> holiday,DateTime date)
        {
            int reday = day;
            for (int i = 0; i < day; i++)
            {
                foreach (var item in holiday)
                {
                    if (date.AddDays(-i).ToString("yyyy-MM-dd") == item.holiday.Value.ToString("yyyy-MM-dd"))
                    {
                        reday = reday + 1;
                    }
                }
            }
            return reday;

        }


